{
    "name": "onlime/exception-report-bundle",
    "description": "Enables simple exception reports by email in your Symfony3 project.",
    "type": "symfony-bundle",
    "keywords": [
        "Onlime",
        "exceptions",
        "error handling",
        "logging",
        "Symfony3",
        "bundle"
    ],
    "homepage": "https://www.onlime.ch/",
    "license": "MIT",
    "authors": [
        {
            "name": "Philip Iezzi",
            "email": "info@onlime.ch"
        },
        {
            "name": "Onlime Webhosting",
            "homepage": "https://github.com/onlime"
        }
    ],
    "require": {
        "php": ">=5.6.0",
        "symfony/dependency-injection": "~3.0",
        "symfony/config": "~3.0",
        "symfony/http-kernel": "~3.0",
        "symfony/twig-bundle": "~3.0",
        "symfony/swiftmailer-bundle": "~2.3",
        "symfony/monolog-bundle": "~2.8",
        "maxmind/geoip": "~v1.1.0"
    },
    "autoload": {
        "psr-4": {
            "Onlime\\ExceptionReportBundle\\": ""
        }
    }
}