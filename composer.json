{
    "name": "onlime/exception-report-bundle",
    "description": "Enables simple exception reports by email in your Symfony3 project.",
    "type": "symfony-bundle",
    "keywords": [
        "logging",
        "exceptions"
    ],
    "homepage": "https://github.com/onlime/ExceptionReportBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Philip Iezzi",
            "email": "info@onlime.ch"
        }
    ],
    "require": {
        "php": ">=5.6.0",
        "symfony/dependency-injection": "~3.0",
        "symfony/config": "~3.0",
        "symfony/http-kernel": "~3.0",
        "symfony/swiftmailer-bundle": "~2.3",
        "maxmind/geoip": "~v1.1.0"
    },
    "autoload": {
        "psr-4": {
            "Onlime\\ExceptionReportBundle\\": ""
        }
    }
}